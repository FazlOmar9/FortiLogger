{
    "manifest_version": 3,
    "name": "FortiLogger",
    "version": "2.0.2",
    "description": "Auto fill Fortigate login form",
    "icons": {
        "48": "./icons/logo48.png",
        "128": "./icons/logo128.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "webRequest",
        "webNavigation"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]   
}